<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:drawable="@drawable/die_base"
        android:id="@+id/die_base"/>
    <item
        android:id="@+id/left_top_eye"
        android:top="10dp"
        android:bottom="70dp"
        android:left="15dp"
        android:right="65dp"
        android:drawable="@drawable/die_eye"/>
