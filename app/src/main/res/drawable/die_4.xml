    <item
        android:id="@+id/right_top_eye"
        android:top="10dp"
        android:bottom="70dp"
        android:left="65dp"
        android:right="15dp"
        android:drawable="@drawable/die_eye"/>
    <item
        android:id="@+id/right_bottom_eye"
        android:top="70dp"
        android:bottom="10dp"
        android:left="65dp"
        android:right="15dp"
        android:drawable="@drawable/die_eye"/>
</layer-list>
